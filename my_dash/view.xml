        <record id="my_dashboard_form" model="ir.ui.view">
            <field name="name"> My DashBoard</field>
            <field name="model">my.dash</field>
            <field name="arch" type="xml">
                <form string="My DashBoard" create="false" delete="false" edit="false">


                    <div class="all_sections">

                        <div class="section green">

                            <h1>Current Members</h1>
                            <field name="name" invisible="1"/>

                            <div class="contains">
                                <p class="texture">Gulberg:</p>
                                
                                <p class="buttoned">
                                    <button name="%(current_member_gulberg)d" type="action" class="oe_stat_button trans" icon="fa-truck">
                                        <field name="current_member_count_gulberg" widget="statinfo" string="Current Members Gulberg"/>
                                    </button>
                                </p>
                            </div>

                            <div class="contains">
                                <p class="texture1">Dha Lahore:</p>
                                
                                <p class="buttoned">
                                    <button name="%(current_member_dha)d" type="action" class="oe_stat_button trans" icon="fa-truck">
                                        <field name="current_member_count_dha" widget="statinfo" string="Current Members Dha Lahore"/>
                                    </button>
                                </p>
                            </div>

                            <div class="contains">
                                <p class="texture2">Johar Town:</p>
                                
                                <p class="buttoned">
                                    <button name="%(current_member_jt)d" type="action" class="oe_stat_button trans" icon="fa-truck">
                                        <field name="current_member_count_jt" widget="statinfo" string="Current Members Johar Town"/>
                                    </button>
                                </p>
                            </div>

                            <div class="contains">
                                <p class="texture3">Dha Karachi:</p>
                                
                                <p class="buttoned">
                                    <button name="%(current_member_kchi)d" type="action" class="oe_stat_button trans" icon="fa-truck">
                                        <field name="current_member_count_kchi" widget="statinfo" string="Current Members Dha Karachi"/>
                                    </button>
                                </p>
                            </div>

                            <div class="contains">
                                <p class="texture4">Total:</p>
                                
                                <p class="buttoned">
                                    <button name="%(current_member)d" type="action" class="oe_stat_button trans" icon="fa-truck">
                                        <field name="current_member_count" widget="statinfo" string="Current Members Dha Karachi"/>
                                    </button>
                                </p>
                            </div>

                            <p class="totaler"></p>

                        </div>

                        <div class="section green">

                            <h1>New Members</h1>

                            <div class="contains">
                                <p class="texture">Gulberg:</p>
                                
                                <p class="buttoned">
                                    <button name="%(new_member_gulberg)d" type="action" class="oe_stat_button trans" icon="fa-truck">
                                        <field name="new_members_count_gulberg" widget="statinfo" string="New Members Gulberg"/>
                                    </button>
                                </p>
                            </div>

                            <div class="contains">
                                <p class="texture1">Dha Lahore:</p>
                                
                                <p class="buttoned">

                                    <button name="%(new_member_dha)d" type="action" class="oe_stat_button trans" icon="fa-truck">
                                        <field name="new_members_count_dha" widget="statinfo" string="New Members Dha Lahore"/>
                                    </button>

                                </p>
                            </div>

                            <div class="contains">
                                <p class="texture2">Johar Town:</p>
                                
                                <p class="buttoned">

                                    <button name="%(new_member_jt)d" type="action" class="oe_stat_button trans" icon="fa-truck">
                                        <field name="new_members_count_jt" widget="statinfo" string="New Members Johar Town"/>
                                    </button>

                                </p>
                            </div>

                            <div class="contains">
                                <p class="texture3">Dha Karachi:</p>
                                
                                <p class="buttoned">

                                    <button name="%(new_member_kchi)d" type="action" class="oe_stat_button trans" icon="fa-truck">
                                        <field name="new_members_count_kchi" widget="statinfo" string="New Members Dha Karachi"/>
                                    </button>

                                </p>
                            </div>

                            <div class="contains">
                                <p class="texture4">Total:</p>
                                
